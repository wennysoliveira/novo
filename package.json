    "preview": "nuxt preview",
    "postinstall": "npx prisma generate && nuxt prepare"
  },
  "dependencies": {
    "@formkit/auto-animate": "^0.9.0",
    "@prisma/client": "^6.18.0",
    "nuxt": "^3.17.7",
    "prisma": "^6.18.0",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@nuxtjs/tailwindcss": "^6.14.0",
    "autoprefixer": "^10.4.21",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.18"
  }
}
