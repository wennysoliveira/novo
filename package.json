{
  "name": "nuxt_santarita",
  "version": "1.0.0",
  "private": true,
  "description": "Projeto Nuxt criado manualmente",
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "prebuild": "npx prisma generate",
    "dev": "nuxt dev",
    "predev": "npx prisma generate",
    "start": "node .output/server/index.mjs",
    "prestart": "npx prisma generate",
    "start:prod": "npx prisma migrate deploy || npx prisma db push && node .output/server/index.mjs",
